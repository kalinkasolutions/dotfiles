Host prox prox.lan
	HostName prox.lan
	User root
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes

Host router router.lan
	HostName router.lan
	User root
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes

Host kalinka-desktop kalinka-desktop.lan
	HostName kalinka-desktop.lan
	User kalinka
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes

Host kalinka-orphis-desktop
	HostName 192.168.1.100
	User kalinka
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes


