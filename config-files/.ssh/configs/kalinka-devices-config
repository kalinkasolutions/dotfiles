Host prox.kalinka.lan
	HostName prox.kalinka.lan
	User root
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes

Host router.kalinka.lan
	HostName router.kalinka.lan
	User root
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes

Host kalinka-desktop.kalinka.lan
	HostName kalinka-desktop.kalinka.lan
	User kalinka
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes

Host kalinka-orphis-desktop
	HostName 192.168.1.100
	User kalinka
	IdentityFile ~/.ssh/kalinka-devices
	IdentitiesOnly yes


